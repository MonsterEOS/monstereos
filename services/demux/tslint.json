{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "no-console": [true, "log"],
    "semicolon": [true, "never"],
    "ter-indent": [true, 2]
  },
  "rulesDirectory": []
}
